<mvc:View controllerName="com.ivancio.zal30-ui5.controller.ViewData" 
    displayBlock="true" 
    xmlns="sap.m" 
    xmlns:l="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:table="sap.ui.table" 
    xmlns:mvc="sap.ui.core.mvc">
    <Page backgroundDesign="Standard" 
        contentOnlyBusy="false" 
        enableScrolling="true" 
        floatingFooter="false" 
        navButtonTooltip="" 
        showFooter="true" 
        showHeader="true" 
        showNavButton="true" 
        navButtonPress="onNavBack" 
        showSubHeader="true" 
        title="{ model: 'ViewData', path:'/viewName' } - { model: 'ViewData', path:'/viewDesc' }" 
        visible="true">
        <content>
            <table:Table
				id="tableData"
				selectionMode="MultiToggle"
				visibleRowCount="10"
				enableSelectAll="false"
				rows="{ model: 'ViewData', path:'/values' }"
				threshold="15"
				enableBusyIndicator="true"
				columns="{
					model: 'ViewData', 
                    path:'/columns',
					factory: '.columnFactory'
				}"
				ariaLabelledBy="title"
                width="100%">				
			</table:Table>
                        <Button press="onTesting"
						icon="sap-icon://navigation-right-arrow" 
						text="Testing"
						enabled="{model:'ViewSelectModel', path:'/viewSelected'}">
						
					</Button>
        </content>
        <customHeader>
            <!--sap.m.IBar-->
        </customHeader>
        <footer>
            <!--sap.m.IBar-->
        </footer>
        <headerContent>
            <!--sap.ui.core.Control-->
        </headerContent>
        <landmarkInfo>
            <!--sap.m.PageAccessibleLandmarkInfo-->
        </landmarkInfo>
        <subHeader>
            <!--sap.m.IBar-->
        </subHeader>
    </Page>
</mvc:View>     