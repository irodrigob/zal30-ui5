<core:FragmentDefinition xmlns="sap.m" 
    xmlns:core="sap.ui.core">
    <Dialog title="{model:'ConfirmDialogModel', path:'/title'}" class="sapUiSizeCompact logDialog" draggable="true">
        <content>
            <Text text="{model:'ConfirmDialogModel', path:'/textMessage'}" class="sapUiSmallMargin"/>
        </content>
        <beginButton>
            <Button text="{model:'ConfirmDialogModel', path:'/beginButtonText'}" 
                press="onBeginButtonDialog"/>
        </beginButton>
        <endButton>
            <Button text="{model:'ConfirmDialogModel', path:'/endButtonText'}" 
                press="onEndButtonDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>