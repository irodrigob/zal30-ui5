<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>viewHelp.title}"
		class="sapUiPopupWithPadding"
		items="{model: 'ViewConf', path: '/viewList'}"
		search="_handleViewHelpSearch"
		confirm="_handleViewHelpClose"
		cancel="_handleViewHelpClose">
		<StandardListItem						
			title="{ViewConf>VIEWNAME}"
			description="{ViewConf>VIEWDESC}" />
	</SelectDialog>
</core:FragmentDefinition>